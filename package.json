{
  "name": "@darkstore/roadie",
  "version": "1.0.30",
  "description": "node library to access the roadie web service api",
  "main": "dist/index.js",
  "husky": {
    "hooks": {
      "pre-commit": "npm run test",
      "pre-push": "npm run build"
    }
  },
  "scripts": {
    "build": "babel src --out-dir ./dist --source-maps",
    "lint:fix": "eslint --fix src test",
    "lint": "eslint src test",
    "setup-semantic-release": "semantic-release-cli setup",
    "test": "mocha --require @babel/register --recursive \"test/**/*.js\" --timeout 5000",
    "test-ci": "NODE_ENV=test nyc mocha --require @babel/register --reporter mocha-junit-reporter --reporter-options mochaFile=reports/mocha/test-results.xml --timeout 30000 --recursive",
    "test-grep": "DEBUG=\"roadie*\" mocha --require @babel/register --recursive \"test/**/*.js\" --timeout 5000 --grep",
    "test-grep-watch": "mocha --require @babel/register --recursive \"test/**/*.js\" --timeout 5000 --watch --grep",
    "test-debug": "DEBUG=\"axios*,roadie*\" mocha --require @babel/register --recursive \"test/**/*.js\" --timeout 5000",
    "test-watch": "mocha --require @babel/register --recursive \"test/**/*.js\" --timeout 5000 --watch"
  },
  "author": "github@iammattwalters.com",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/plugin-transform-runtime": "^7.5.0",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.4",
    "@babel/runtime": "^7.5.4",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "husky": "^2.3.0",
    "mocha": "^6.1.4",
    "mocha-junit-reporter": "^1.22.0",
    "moment": "^2.24.0",
    "nyc": "^14.1.1",
    "prettier": "^1.17.1",
    "semantic-release-cli": "^5.2.0"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "debug": "^4.1.1"
  },
  "files": [
    "/dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/darkstoregh/roadie.git"
  },
  "publishConfig": {
    "registry": "http://nexus-jx.104.198.21.14.nip.io/repository/npm-group/"
  }
}
